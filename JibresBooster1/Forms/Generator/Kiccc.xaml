        mc:Ignorable="d"
        Title="تست کارتخوان ایران کیش" Height="300" Width="650" ResizeMode="NoResize" Icon="/JibresBooster1;component/favicon.ico" FlowDirection="RightToLeft" Initialized="Window_Initialized" KeyUp="Window_KeyUp" WindowStartupLocation="CenterScreen">
